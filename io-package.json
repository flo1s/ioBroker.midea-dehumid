{
  "common": {
    "name": "midea-dehumid",
    "version": "0.1.0",
    "title": "Midea Dehumidifier",
    "titleLang": {
      "en": "Midea Dehumidifier",
      "de": "Midea Entfeuchter"
    },
    "desc": {
      "en": "Midea Adapter for midea, dimstal or royal clima or Carrier",
      "de": "Midea Adapter für midea, dimstal oder royal clima oder Carrier Klimaanlagen und Wärmepumpen"
    },
    "authors": [
      "flo1s <xxx>"
    ],
    "keywords": [
      "midea",
      "dimstal",
      "royal clima",
      "Carrier"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "main": "main.js",
    "icon": "midea.png",
    "extIcon": "https://raw.githubusercontent.com/TA2k/ioBroker.midea/master/admin/midea.png",
    "readme": "https://github.com/TA2k/ioBroker.midea/blob/master/README.md",
    "loglevel": "info",
    "mode": "daemon",
    "type": "climate-control",
    "compact": true,
    "connectionType": "cloud",
    "dataSource": "poll",
    "materialize": true,
    "admin": {
      "type": "json",
      "jsonConfig": true,
      "supportCustoms": false,
      "config": {
        "type": "object",
        "properties": {
          "user": {
            "type": "string",
            "title": "Benutzername / E-Mail"
          },
          "password": {
            "type": "string",
            "title": "Passwort"
          },
          "type": {
            "type": "string",
            "title": "Verbindungstyp",
            "enum": ["nethome", "cloud"],
            "default": "nethome"
          },
          "local": {
            "type": "boolean",
            "title": "Lokale Verbindung verwenden",
            "default": false
          },
          "interval": {
            "type": "number",
            "title": "Abrufintervall in Minuten",
            "default": 5
          }
        },
        "required": ["user", "password", "type"]
      }
    },
    "plugins": {
      "sentry": {
        "dsn": "https://dd8e8ba4255b4def8c9f9c220a403e94@sentry.iobroker.net/78"
      }
    },
    "dependencies": [
      {
        "js-controller": ">=3.2.0"
      }
    ],
    "globalDependencies": [
      {
        "admin": ">=5.2.28"
      }
    ]
  },
  "encryptedNative": ["password"],
  "protectedNative": ["password"],
  "native": {
    "user": "",
    "password": "",
    "type": "nethome",
    "local": false,
    "interval": 5
  },
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}
